# Example .env file for Misoto CLI
# Copy this to .env and adjust values as needed

# AI Provider Configuration
ANTHROPIC_API_KEY=sk-ant-api03-your-key-here
MISOTO_AI_DEFAULT_PROVIDER=ollama

# Ollama Configuration (if using Ollama)
OLLAMA_HOST=http://localhost:11434
MISOTO_AI_OLLAMA_URL=http://localhost:11434
MISOTO_AI_OLLAMA_MODEL=qwen2.5:0.5b

# Agent System Configuration
MISOTO_AGENT_MODE=true
MISOTO_AGENT_MAX_TASKS=3
MISOTO_AGENT_INTERVAL=5000